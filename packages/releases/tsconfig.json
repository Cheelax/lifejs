{
  "compilerOptions": {
    "target": "ES2020",

    "module": "ES2020",
    "moduleResolution": "node",

    "outDir": "./",
    "declaration": false,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "rootDir": "./"
  },
  "include": ["formatter.ts"],
  "exclude": ["node_modules"]
}
